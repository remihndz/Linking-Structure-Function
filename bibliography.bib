  shorttitle = {Ocular perfusion pressure and glaucoma},
  uurl        = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2662722/},
  uurldate    = {2023-06-21},
}

@Article{Rudnicka2022,
  author    = {Rudnicka, Alicja Regina and Welikala, Roshan and Barman, Sarah and Foster, Paul J. and Luben, Robert and Hayat, Shabina and Khaw, Kay-Tee and Whincup, Peter and Strachan, David and Owen, Christopher G.},
  journal   = {Br J Ophthalmol},
