  file     = {Arthur et al_2019_Distances From Capillaries to Arterioles or Venules Measured Using OCTA and.pdf:C\:\\Users\\rhernand\\Desktop\\Papers\\Arthur et al_2019_Distances From Capillaries to Arterioles or Venules Measured Using OCTA and.pdf:application/pdf},
  keywords = {notion},
  pmcid    = {PMC6892442},
